<!doctype html>
<html lang="en">

    <head>
        
        <meta charset="utf-8" />
        <title>Viajes | wTracking BackOffice</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta content="BackOffice wTracking" name="description" />
        <meta content="AGL, LTD" name="author" />
        <!-- App favicon -->
        <link rel="shortcut icon" href="public/assets/images/favicon.ico?v=1.0.1">
        <meta name="robots" content="noindex, nofollow" />
        <%- include ('../Partials/HeaderStyle') %>
        <link href="public/assets/css/app.tracking.css" id="app-style" rel="stylesheet" type="text/css" />
    </head>

    <body class="auth-body-bg">
       <input type="hidden" id="app_tracking_unit" value="<%= app_form_data.vehicle_id %>">
       <input type="hidden" id="app_tracking_company" value="<%= app_form_data.vehicle_company %>">
        <div>
            <div class="container-fluid p-0">
                <div class="col-lg-12">
                    <div class="card">
                        <div class="card-body">
                            <div class="header-tracking-content">
                                <div class="row">
                                    <div class="col-md-2">
                                        <img class="logo-tracking-unit" src="public/assets/images/logo-dark.png">
                                    </div>
                                    <div class="col-md-10">
                                        <div class="header-tracking-unit-info">
                                            <div class="row">
                                                <div class="col-md-1"><label class="tracking-top-text"><%= app_form_data.vFicha %></label></div>
                                                <div class="col-md-1"><label class="tracking-top-text unit_speed">0 km</label></div>
                                                <div class="col-md-8"><label class="tracking-top-text unit_address">N/A</label></div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-1"><label class="tracking-bottom-text">ficha</label></div>
                                                <div class="col-md-1"><label class="tracking-bottom-text">velocidad</label></div>
                                                <div class="col-md-8"><label class="tracking-bottom-text">Ubicacion</label></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div id="map" class="gmaps"></div>
                        </div>
                    </div>
                </div> 
            </div>
        </div>

        <script src="public/assets/libs/jquery/jquery.min.js"></script>
        <script src="public/assets/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
        <script src="public/assets/libs/metismenu/metisMenu.min.js"></script>
        <script src="public/assets/libs/simplebar/simplebar.min.js"></script>
        <script src="public/assets/libs/node-waves/waves.min.js"></script>
       
        <!-- google maps api -->
        
        <script>
            let app_tracking_unit=$("#app_tracking_unit").val();
            let app_tracking_company=$("#app_tracking_company").val();
            let app_token_id='<%= app_form_data.token_id %>';
            // var userId= '<%= process.env.userId %>';
            let app_url_api='<%= process.env.VUE_APP_WS_URL %>';
        </script>
        <!-- Gmaps file -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.3.1/socket.io.js"></script>        
        <script src="public/assets/js/pages/adm-tracking.init.js"></script>
        <script src="https://maps.google.com/maps/api/js?v=3&key=AIzaSyDJAmKIJZDYB_H6lTEv6VHS1YZjc2qR9HE&callback=initMap"></script>

    </body>
</html>